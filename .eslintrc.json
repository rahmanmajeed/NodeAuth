{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "error",
    "no-unused-vars": "off",
    "no-unused-expressions": "off",
    "arrow-body-style": "off",
    "no-param-reassign": "off",
    "consistent-return": "off"
  }
}
